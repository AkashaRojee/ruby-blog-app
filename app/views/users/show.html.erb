<%= render partial: 'user', object: @user %>

<div class="user-bio flex-col">
  <h3>Bio</h3>
  <%= @user.bio %>
</div>

<% @posts.each do |post| %>
  Post # <%= post.id %><br><br>
  <%= post.text %><br><br>
  Comments: <%= post.comments_counter %> Likes: <%= post.likes_counter %><br><br>
<% end %>

<%= link_to "See all posts" %>